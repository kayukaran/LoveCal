<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kayu.loveapps"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="15" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainPage"
            android:label="@string/title_activity_main_page" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SecoundPage"
            android:label="@string/title_activity_secound_page" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.kayu.loveapps.MainPage" />
        </activity>
        <activity
            android:name=".ProgrssActivi"
            android:label="@string/title_activity_progrss" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.kayu.loveapps.MainPage" />
        </activity>
    </application>

</manifest>